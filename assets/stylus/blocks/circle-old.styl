.circle-old
  width 94px
  height 94px
  float left
  border-radius 100px
  text-align center
  margin 0 20px 0 20px
  &:hover
    border-width 5px

  &--light-green
    border 3px solid #a8d53c
  &--green
    border 3px solid #7cbb22
  &--red
    border 3px solid #e7582a
  &--orange
    border 3px solid #fd7900
  &--blue
    border 3px solid #00a2e5
  &--light-blue
    border 3px solid #00c7df
